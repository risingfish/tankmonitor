{
  "carrier": "verizon",
  "notify": {
    "waterLevel": [
      "555555555"
    ],
    "temperature": []
  },
  "messages": {
    "waterLevel": "The pond water level is low!"
  },
  "sensors": {
    "waterLevel": {
      "pin": 17,
      "readInterval": 1000,
      "handleRead": "function () {}"
    },
    "temperature": {
      "readInterval": 1000,
      "handleRead": "function () {}"
    }
  },
  "mailRelay": {
    "hostname": "YOUR_MAIL_RELAY_HOST",
    "port": 25,
    "secure": false,
    "senderAddress": "\"YOUR_NAME\" <EMAIL_ADDRESS>"
  },
  "dataHandling": {
    "dataFilePath": "URL", // Path to write dataFileName into
    "dataFileName": "FILENAME", // dataFileName file name
    "append": false
  },
  "dev": true // If this is present, SMS messages will not be sent. Delete this line to send SMS.
}